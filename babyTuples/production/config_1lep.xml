	Algo="selectedPatTaus" 
    PtThreshold="-1" EtaThreshold="-1" RelIso="-1" D0Cut="-1" VertexMatchThr="-1" TauLeadTrkPtCut="-1"
/>
<s 
	type="Vertex"   
	Loaded="1" 
	Algo="goodVertices" 
    VertexNdofThr="-1" VertexZThr="-1" VertexRhoThr="-1" 
/>
<s 
	type="PFCandidate"
	Loaded="1"
	Algo="particleFlow"
/>
